{
    "manifest_version": 3,
    "name": "Video Repeat Extension",
    "description": "Extension to repeat video segments a specified number of times.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
      
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  